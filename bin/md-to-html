#!/bin/sh

set -eu -o pipefail

if [ $# -eq 0 ]; then
    echo "Usage: $0 <path-to-interpolated-md-file>"
    exit 1
fi

interpolated_path="$1"

cat "$interpolated_path" \
  | pandoc --from markdown \
           --to html5 \
           --standalone \
           --css '' \
  > public/resume/index.html
